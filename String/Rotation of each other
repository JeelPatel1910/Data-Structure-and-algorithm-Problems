// Java program to check if two given strings are rotations of each other
class StringRotationOfOthers
{
   static boolean areRotations(String str1 , String str2)
   {
	    return(str1.length()== str2.length())&&( (str1+str1).indexOf(str2)!=-1);
    
	}
public static void main(String args[])
{ 
   String str1 = "WahKya";
   String str2 = "KyaWah";
   if(areRotations(str1,str2))
      System.out.println("String are rotation of each other");
   else
       System.out.println("String are not rotation of each other");
 }
 }
